# Exchange API Keys (Optional - users can add via Settings)
BYBIT_API_KEY=your_bybit_api_key_here
BYBIT_API_SECRET=your_bybit_api_secret_here
BYBIT_DEMO=True

# Telegram Notifications (Optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# JWT Configuration (REQUIRED)
JWT_SECRET_KEY=generate_with_openssl_rand_hex_32

# Encryption Key (Generated using generate_encryption_key.py - REQUIRED)
ENCRYPTION_KEY=test

# CORS Configuration (comma-separated)
# DEVELOPMENT: Use * or specific localhost ports
# PRODUCTION: MUST specify exact domains (e.g., https://myapp.com). Do NOT use * in production.
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Environment Configuration
# Set to "production" to enable HTTPS enforcement and strict security
# Set to "development" for local development without HTTPS
ENVIRONMENT=development

# Optional: External Services
GEMINI_API_KEY=your_gemini_api_key_here
CRYPTOPANIC_API_KEY=your_cryptopanic_api_key_here
COINBASE_COMMERCE_API_KEY=your_coinbase_commerce_key_here
COINBASE_COMMERCE_WEBHOOK_SECRET=your_webhook_secret_here

# News API Keys (Optional - Free tiers available)
# Get free API keys from:
# - Alpha Vantage: https://www.alphavantage.co/support/#api-key
# - Finnhub: https://finnhub.io/register
# - Marketaux: https://www.marketaux.com/
